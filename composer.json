{
    "name": "flux-se/odoo-api-client",
    "description": "Odoo API client library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "francis@flux.audio"
        }
    ],
    "require": {
        "php": "^8.0",
        "php-http/httplug": "^1.0|^2.0",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.3|^2.1",
        "php-http/discovery": "^1.7",
        "psr/http-factory-implementation": "^1.0",
        "symfony/console": "^5.4|^6.3",
        "symfony/serializer": "^5.4",
        "symfony/property-access": "^5.4|^6.3",
        "symfony/property-info": "^5.4|^6.3",
        "symfony/string": "^5.4|^6.3",
        "symfony/http-kernel": "^5.4|^6.3",
        "php-http/logger-plugin": "^1.1",
        "prometee/php-class-generator": "^1.0",
        "doctrine/annotations": "^1.13",
        "webmozart/assert": "^1"
    },
    "require-dev": {
        "ext-xmlrpc": "*",
        "ext-json": "*",
        "phpunit/phpunit": "^9",
        "php-http/guzzle7-adapter": "^1.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "symplify/easy-coding-standard": "^11",
        "phpstan/phpstan": "^1",
        "vimeo/psalm": "^4.18"
    },
    "suggest": {
        "ext-xmlrpc": "Needed to support XML-RPC.",
        "ext-json": "Needed to support JSON-RPC."
    },
    "bin": [
        "bin/odoo-model-classes-generator"
    ],
    "autoload": {
        "psr-4": { "FluxSE\\OdooApiClient\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\FluxSE\\OdooApiClient\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
